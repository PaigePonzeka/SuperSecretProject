%h1 Listing places
%table
  %tr
    %th Name
    %th Street
    %th City
    %th Zipcode
    %th Phone
    %th Yelp
    %th
    %th
    %th
  - @places.each do |place|
    %tr
      %td= place.name
      %td= place.street
      %td= place.city
      %td= place.zipcode
      %td= place.phone
      %td= place.yelp
      %td= link_to 'Show', place
      %td= link_to 'Edit', edit_place_path(place)
      %td= link_to 'Destroy', place, :confirm => 'Are you sure?', :method => :delete
%br/
= link_to 'New Place', new_place_path
