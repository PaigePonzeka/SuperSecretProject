= form_for(@round) do |f|
  - if @round.errors.any?
    #error_explanation
      %h2
        = pluralize(@round.errors.count, "error")
        prohibited this round from being saved:
      %ul
        - @round.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :group_id
    %br/
    = f.text_field :group_id
  .field
    = f.label :name
    %br/
    = f.text_field :name
  .field
    = f.fields_for :votes do |v|
      = " here"
      %p
        Vote:
        = v.text_field :tally
  .actions
    = f.submit
